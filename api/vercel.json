{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "https://vercel-app-api-three.vercel.app/api/",
      "headers": {
        "Access-Control-Allow-Origin": "https://vercel-app-client-ten.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE"
      }
    }
  ]
}
